{
  "name": "merchant-health-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend API for Merchant Health Dashboard",
  "main": "src/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "kill": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true && pkill -f 'nodemon\\|ts-node' 2>/dev/null || true",
    "debug": "npm run kill && nodemon --exec \"node --inspect=0.0.0.0:9229 -r ts-node/register src/app.ts\"",
    "debug:brk": "npm run kill && nodemon --exec \"node --inspect-brk=0.0.0.0:9229 -r ts-node/register src/app.ts\"",
    "debug:vscode": "npm run kill && ts-node --inspect=9229 src/app.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "helmet": "^7.0.0",
    "influx": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.10.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.8.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "keywords": [
    "dashboard",
    "influxdb",
    "merchant",
    "health"
  ],
  "author": "Arshad Khan",
  "license": "MIT"
}
